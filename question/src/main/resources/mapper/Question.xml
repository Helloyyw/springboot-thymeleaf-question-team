<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.question.question.dao.question.QuestionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.question.question.bean.question.Question">
        <id column="bsm" property="bsm" />
        <result column="typeBsm" property="typeBsm" />
        <result column="questionTtile" property="questionTtile" />
        <result column="isSigle" property="isSigle" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        bsm, typeBsm, questionTtile, isSigle
    </sql>

</mapper>
